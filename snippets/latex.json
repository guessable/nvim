{
  "Head": {
		"prefix": "head",
		"body": [
			"%!TEX program=xelatex",
			"%      \"   ",
			"%    '':''",
			"%   ___:____      |\\/|",
			"% ,'        `.    \\  /",
			"% |  O        \\___/  |",
			"% ~^~^~^~^~^~^~^~^~^~^",
			"$0"
		],
		"description": "Head"
	},
  "beg": {
		"prefix": "begin",
		"body": [
      "\\begin{$1}",
      "\t$0",
      "\\end{$1}"
		],
		"description": "begin...end"
	},
  "equation": {
		"prefix": "BEQ",
		"body": [
      "\\begin{equation}",
      "\t$0",
      "\\end{equation}"
		],
		"description": "equation"
	},
  "equation*": {
		"prefix": "BSEQ",
		"body": [
      "\\begin{equation*}",
      "\t$0",
      "\\end{equation*}"
		],
		"description": "equation*"
	},
